{
  "name": "spam-tweets-compressor",
  "version": "2.0.4",
  "description": "A browser extension which compresses spam Tweets.",
  "main": "main.js",
  "scripts": {
    "test": "npx jest",
    "build": "npx webpack",
    "watch": "npx webpack -w",
    "lint:js": "npx eslint src/**/*.ts",
    "lint:css": "stylelint **/*.{css,scss,sass}",
    "format": "npx prettier --write \"**/*.{js,ts,css}\"",
    "pack": "npx web-ext build --ignore-files \"./**/*.md\" \"./src/\" \"./docs/\" \"./gh-pages/\" \"./image/for_readme/\" package.json package-lock.json \"tsconfig*.json\" webpack.config.js \"./image/logo.*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robot-Inventor/spam-tweets-compressor.git"
  },
  "author": "Robot-Inventor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robot-Inventor/spam-tweets-compressor/issues"
  },
  "homepage": "https://github.com/Robot-Inventor/spam-tweets-compressor#readme",
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.1.0",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "typescript": "4.4.2",
    "web-ext": "6.3.0",
    "web-ext-types": "3.2.1",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0"
  }
}