{
  "name": "spam-tweets-compressor",
  "version": "2.0.8",
  "description": "A browser extension which compresses spam Tweets.",
  "main": "main.js",
  "scripts": {
    "test": "npx jest",
    "build:css": "npx sass src/css:dist --style=compressed --update",
    "build:js": "npx webpack",
    "build": "npm-run-all --parallel build:*",
    "watch:js": "npx webpack -w",
    "watch:css": "npx sass src/css:dist --style=compressed --watch",
    "watch": "npm-run-all --parallel watch:*",
    "lint:js": "npx eslint src/**/*.ts",
    "lint:css": "stylelint src/**/*.{css,scss,sass}",
    "lint": "npm-run-all --parallel lint:*",
    "format": "npx prettier --write \"src/**/*.{js,ts,css,scss}\"",
    "pack": "npx web-ext build --ignore-files \"./**/*.md\" \"./src/\" \"./docs/\" \"./gh-pages/\" \"./image/for_readme/\" package.json package-lock.json \"tsconfig*.json\" webpack.config.js \"./image/logo.*\"",
    "generate-schema": "npx typescript-json-schema --noExtraProps --required --strictNullChecks \"src/ts/types/common/setting.ts\" \"setting_object\" -o \"src/ts/types/common/setting.schema.json\"",
    "compile-validator": "npx ajv compile -s \"src/ts/types/common/setting.schema.json\" -o \"src/ts/types/common/setting.validate.js\"",
    "update-validator": "npm-run-all --serial generate-schema compile-validator",
    "version": "node ./src/script/version.js && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robot-Inventor/spam-tweets-compressor.git"
  },
  "author": "Robot-Inventor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robot-Inventor/spam-tweets-compressor/issues"
  },
  "homepage": "https://github.com/Robot-Inventor/spam-tweets-compressor#readme",
  "devDependencies": {
    "@material/mwc-button": "0.26.1",
    "@material/mwc-checkbox": "0.26.1",
    "@material/mwc-circular-progress": "0.26.1",
    "@material/mwc-dialog": "0.26.1",
    "@material/mwc-drawer": "0.26.1",
    "@material/mwc-formfield": "0.26.1",
    "@material/mwc-icon": "0.26.1",
    "@material/mwc-icon-button": "0.26.1",
    "@material/mwc-list": "0.26.1",
    "@material/mwc-ripple": "0.26.1",
    "@material/mwc-switch": "0.26.1",
    "@material/mwc-tab-bar": "0.26.1",
    "@material/mwc-textarea": "0.26.1",
    "@material/mwc-top-app-bar-fixed": "0.26.1",
    "@types/chroma-js": "2.1.3",
    "@types/deep-diff": "1.0.1",
    "@types/dot-object": "2.1.2",
    "@types/jest": "28.1.3",
    "@typescript-eslint/eslint-plugin": "5.30.4",
    "@typescript-eslint/parser": "5.30.4",
    "ajv": "8.11.0",
    "ajv-cli": "5.0.0",
    "chroma-js": "2.4.2",
    "css-loader": "6.7.1",
    "deep-diff": "1.0.2",
    "deepmerge": "4.2.2",
    "dot-object": "2.1.4",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.1",
    "normalize-url": "7.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "sass": "1.53.0",
    "style-loader": "3.3.1",
    "stylelint": "14.9.1",
    "stylelint-config-recommended-scss": "6.0.0",
    "stylelint-scss": "4.2.0",
    "tippy.js": "6.3.7",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "typescript-json-schema": "0.53.1",
    "url2emoji": "1.0.3",
    "web-ext": "7.1.0",
    "web-ext-types": "3.2.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  }
}
